pp.js",[],"/Users/seanadamson/Library/Mobile Documents/com~apple~CloudDocs/React Projects /ecommerce-app/src/navigation-links/navigation-links.js",[],"/Users/seanadamson/Library/Mobile Documents/com~apple~CloudDocs/React Projects /ecommerce-app/src/components/slider/slider.jsx",[],"/Users/seanadamson/Library/Mobile Documents/com~apple~CloudDocs/React Projects /ecommerce-app/src/components/menu-item/menu-item.jsx",[],"/Users/seanadamson/Library/Mobile Documents/com~apple~CloudDocs/React Projects /ecommerce-app/src/components/navigation-bar/navigation-bar.jsx",["27","28"],"import React from 'react';\nimport './navigation-bar.scss';\nimport { withRouter } from 'react-router-dom';\nimport logo from '../../assets/logo2.png';\nimport user from '../../assets/001-user.png';\nimport favourites from '../../assets/004-heart-shape-outline.png';\nimport shopping from '../../assets/002-shopping-bag.png';\nimport closebutton from '../../assets/close-button.png';\nimport mobile from '../../assets/menu.png';\n\nclass NavigationBar extends React.Component {\n  state = {\n    mensOpen: false,\n    womensOpen: false,\n    width: 0,\n  };\n  handleOpen = (e) => {\n    if (e.target.id === 'womens') {\n      this.setState({\n        womensOpen: true,\n        mensOpen: false,\n      });\n    }\n\n    if (e.target.id === 'mens') {\n      this.setState({\n        mensOpen: true,\n        womensOpen: false,\n      });\n    }\n\n    if (e.target.id === 'mobile') {\n      this.setState({\n        width: 100,\n      });\n    }\n  };\n  handleClose = () => {\n    this.setState({\n      mensOpen: false,\n      womensOpen: false,\n      width: 0,\n    });\n  };\n  renderLinks = () => {\n    if (this.state.mensOpen) {\n      return {\n        apparelLinks: this.props.mensApparelLinks,\n        shoeLinks: this.props.mensShoeLinks,\n        collectionLinks: this.props.mensCollectionLinks,\n      };\n    } else if (this.state.womensOpen) {\n      return {\n        apparelLinks: this.props.womensApparelLinks,\n        shoeLinks: this.props.womensShoeLinks,\n        collectionLinks: this.props.womensCollectionLinks,\n      };\n    }\n  };\n  render() {\n    const { mensOpen, womensOpen, width } = this.state;\n    const linksToRender = this.renderLinks();\n    const { history } = this.props;\n    return (\n      <React.Fragment>\n        <div style={{ width: `${width}vw` }} className='mobile-menu'>\n          <div className='mobile-menu__content-container'>\n            <div className='mobile-menu__top-buttons'>\n              <img src={closebutton} onClick={this.handleClose} />\n              <div onClick={() => history.push('/')} className='nav__logo'>\n                <img src={logo} alt='logo' />\n                <div className='nav__logo-text'>\n                  <p>Faith</p>\n                  <p>Apparel</p>\n                </div>\n              </div>\n              <img src={shopping} alt='shopping bag' />\n            </div>\n            <div className='mobile-menu__content'></div>\n          </div>\n        </div>\n        <nav className='nav'>\n          <div className='nav__menu'>\n            <ul className='nav__menu-list'>\n              <li\n                id='mens'\n                onClick={this.handleOpen}\n                className={\n                  mensOpen\n                    ? `nav__menu-list-item active`\n                    : `nav__menu-list-item`\n                }\n              >\n                Men\n              </li>\n              <li\n                id='womens'\n                onClick={this.handleOpen}\n                className={\n                  womensOpen\n                    ? `nav__menu-list-item active`\n                    : `nav__menu-list-item`\n                }\n              >\n                Women\n              </li>\n              <li className='nav__menu-list-item'>Collections</li>\n            </ul>\n          </div>\n          <div onClick={() => history.push('/')} className='nav__logo'>\n            <img src={logo} alt='logo' />\n            <div className='nav__logo-text'>\n              <p>Faith</p>\n              <p>Apparel</p>\n            </div>\n          </div>\n          <div className='nav__user-menu'>\n            <div className='nav__user-menu-item'>\n              <img\n                onClick={() => history.push('/signin')}\n                src={user}\n                alt='user login'\n              />\n              <div className='account__menu'>\n                <button onClick={() => alert(1)}>SIGN IN</button>\n                <p onClick={() => alert(3)}>Not registered? Sign up!</p>\n              </div>\n            </div>\n            <div className='nav__user-menu-item'>\n              <img src={favourites} alt='favourites' />\n            </div>\n            <div className='nav__user-menu-item'>\n              <img src={shopping} alt='shopping bag' />\n            </div>\n          </div>\n          <div className='nav__mobile-menu'>\n            <img\n              id='mobile'\n              onClick={this.handleOpen}\n              src={mobile}\n              alt='mobile menu'\n            />\n          </div>\n        </nav>\n        <div className='dropdown' onClick={this.handleClose}>\n          <div\n            className={\n              mensOpen || womensOpen ? `dropdown__menu open` : `dropdown__menu`\n            }\n          >\n            <div\n              className={\n                mensOpen\n                  ? 'dropdown__menu-content open'\n                  : womensOpen\n                  ? 'dropdown__menu-content open'\n                  : 'dropdown__menu-content'\n              }\n            >\n              <div className='dropdown__column'>\n                {linksToRender &&\n                  linksToRender.apparelLinks.map(({ path, category }) => (\n                    <p onClick={() => history.push(path)} key={path}>\n                      {category}\n                    </p>\n                  ))}\n              </div>\n              <div className='dropdown__column'>\n                {linksToRender &&\n                  linksToRender.shoeLinks.map(({ path, category }) => (\n                    <p onClick={() => history.push(path)} key={path}>\n                      {category}\n                    </p>\n                  ))}\n              </div>\n              <div className='categories'>\n                {linksToRender &&\n                  linksToRender.collectionLinks.map(\n                    ({ path, category, img }) => (\n                      <div onClick={() => history.push(path)} key={path}>\n                        {img && <img src={img} />}\n                        <p>{category}</p>\n                      </div>\n                    )\n                  )}\n              </div>\n            </div>\n          </div>\n        </div>\n        <div className={mensOpen || womensOpen ? 'overlay open' : ''}></div>\n      </React.Fragment>\n    );\n  }\n}\n\nexport default withRouter(NavigationBar);\n","/Users/seanadamson/Library/Mobile Documents/com~apple~CloudDocs/React Projects /ecommerce-app/src/index.js",[],{"ruleId":"29","severity":1,"message":"30","line":69,"column":15,"nodeType":"31","endLine":69,"endColumn":67},{"ruleId":"29","severity":1,"message":"30","line":181,"column":33,"nodeType":"31","endLine":181,"endColumn":50},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]