ecommerce-app/src/components/favourites/favourites-list/favourites-list.jsx",[],["229","230"],"/Users/seanadamson/Documents/ecommerce-app/src/App.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/pages/auth/register/register.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/redux/reducers/productReducer.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/main-menu/main-menu.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/filters/colors.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/redux/actions/favouriteActions.js",[],["231","232"],"/Users/seanadamson/Documents/ecommerce-app/src/components/loading/loading.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/menu-item/menu-item.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/redux/types.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/filters/bestfor.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/redux/actions/cartActions.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/utils/local-storage/cart-handler.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/slider/slider-image.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/cart/cart-item/cart-item.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/cart/cart-list/cart.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/utils/text-formatting/text-formatting.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/pages/products/product/product-page.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/products/product/product-main.jsx",["233"],"import React from \"react\";\nimport \"./product-main.scss\";\nimport { capitaliseFirstLetter } from \"../../../utils/text-formatting/text-formatting\";\nimport Accordion from \"../../../components/accordion/accordion\";\nimport \"../../../components/accordion/accordion.scss\";\n\nclass ProductMain extends React.Component {\n  state = {\n    index: 0,\n    selectedSize: null,\n    productFeatures: [],\n  };\n\n  componentDidMount = () => {\n    this.setState({\n      productFeatures: this.props.product.features\n        ? [\n            {\n              title: \"core features\",\n              content: this.props.product.features,\n              open: false,\n            },\n            {\n              title: \"description\",\n              content: this.props.product.description,\n              open: false,\n            },\n            {\n              title: \"delivery & returns\",\n              content: this.props.product.delivery,\n              open: false,\n            },\n            { title: \"care\", content: this.props.product.care, open: false },\n          ]\n        : null,\n    });\n  };\n\n  componentDidUpdate = (prevProps, prevState) => {\n    if (prevState.productFeatures === null) {\n      this.setState({\n        productFeatures: [\n          {\n            title: \"core features\",\n            content: this.props.product.features,\n            open: false,\n          },\n          {\n            title: \"description\",\n            content: this.props.product.description,\n            open: false,\n          },\n          {\n            title: \"delivery & returns\",\n            content: this.props.product.delivery,\n            open: false,\n          },\n          { title: \"care\", content: this.props.product.care, open: false },\n        ],\n      });\n    }\n  };\n\n  handleVariantSelect = (index) => {\n    this.props.handleVariantSelect(index);\n  };\n\n  handleSizeSelect = (size) => {\n    if (this.state.selectedSize === size) {\n      this.setState({\n        selectedSize: null,\n      });\n    } else {\n      this.setState({\n        selectedSize: size,\n      });\n    }\n  };\n\n  handleToggle = (index) => {\n    this.setState((prevState) => ({\n      productFeatures: prevState.productFeatures.map((feature, i) => {\n        if (index === i) {\n          return {\n            ...feature,\n            open: !feature.open,\n          };\n        } else {\n          return {\n            ...feature,\n            open: false,\n          };\n        }\n      }),\n    }));\n  };\n\n  render() {\n    const { product, variantIndex } = this.props;\n    const { selectedSize, productFeatures } = this.state;\n    if (product.colors === undefined || product.variants === undefined) {\n      return <h1>LOADING</h1>;\n    } else {\n      return (\n        <div className=\"product-main\">\n          <h1 className=\"product-main__title\">{product.name}</h1>\n          <p className=\"product-main__price\">£{product.price}</p>\n          <div className=\"product-main__variants-container\">\n            <span>STANDARD EDITION:</span> {\"\"}\n            <span>{capitaliseFirstLetter(product.colors[variantIndex])}</span>\n            <div className=\"product-main__variants\">\n              {product.colors.map((color, index) => (\n                <div\n                  key={index}\n                  className={\n                    variantIndex === index\n                      ? \"product-main__variant active\"\n                      : \"product-main__variant\"\n                  }\n                >\n                  <div\n                    className={`product-main__variant-color product-main__variant-color--${color}`}\n                    onClick={() => this.handleVariantSelect(index)}\n                  />\n                </div>\n              ))}\n            </div>\n          </div>\n          <div className=\"product-main__variant-sizes-container\">\n            <span>SELECT SIZE:</span>\n            <div className=\"product-main__variant-sizes\">\n              {product.variants[variantIndex].sizes &&\n                product.variants[variantIndex].sizes.map((size, index) => (\n                  <div\n                    key={index}\n                    onClick={() => this.handleSizeSelect(size.size)}\n                    className={\n                      select