ountReducer.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/filters/filters.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/nav-dropdown/nav-dropdown.jsx",["251"],"import React from \"react\";\nimport \"./nav-dropdown.scss\";\nimport { withRouter, Link } from \"react-router-dom\";\nimport { CSSTransition } from \"react-transition-group\";\n\nconst NavDropdown = ({\n  handleClose,\n  mensDesktopOpen,\n  womensDesktopOpen,\n  linksToRender,\n  history,\n}) => {\n  return (\n    <React.Fragment>\n      <CSSTransition\n        unmountOnExit\n        mountOnEnter\n        in={mensDesktopOpen || womensDesktopOpen}\n        classNames=\"fade\"\n        timeout={400}\n      >\n        <div className=\"dropdown__overlay\" />\n      </CSSTransition>\n      <CSSTransition\n        unmountOnExit\n        mountOnEnter\n        in={mensDesktopOpen || womensDesktopOpen}\n        classNames=\"drop\"\n        timeout={{\n          enter: 200,\n          exit: 1000,\n        }}\n      >\n        <div id=\"close-dropdown\" className=\"nav-dropdown\" onClick={handleClose}>\n          <div className=\"nav-dropdown__column\">\n            {linksToRender &&\n              linksToRender.apparelLinks.map(({ path, title }, i) => (\n                <Link to={path} key={i}>\n                  {title}\n                </Link>\n              ))}\n          </div>\n          <div className=\"nav-dropdown__column\">\n            {linksToRender &&\n              linksToRender.shoeLinks.map(({ path, title }, i) => (\n                <Link to={path} key={i}>\n                  {title}\n                </Link>\n              ))}\n          </div>\n          <div className=\"categories\">\n            {linksToRender &&\n              linksToRender.collectionLinks.map(({ path, title, img }, i) => (\n                <div key={i}>\n                  {img && <img src={img} />}\n                  <Link className='category-link' to={path}>{title}</Link>\n                <