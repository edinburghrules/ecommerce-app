erce-app/src/components/cart/cart-list/cart.jsx",["310","311"],"/Users/seanadamson/Documents/ecommerce-app/src/utils/text-formatting/text-formatting.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/pages/products/product/product-page.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/products/product/product-main.jsx",["312","313"],"import React from \"react\";\nimport \"./product-main.scss\";\nimport { withRouter } from \"react-router-dom\";\nimport { capitaliseFirstLetter } from \"../../../utils/text-formatting/text-formatting\";\nimport Accordion from \"../../../components/accordion/accordion\";\nimport \"../../../components/accordion/accordion.scss\";\nimport { connect } from \"react-redux\";\nimport { addToCart } from \"../../../redux/actions/cartActions\";\n\nclass ProductMain extends React.Component {\n  state = {\n    index: 0,\n    selectedSize: null,\n    productFeatures: [],\n  };\n\n  componentDidMount = () => {\n    this.setState({\n      productFeatures: this.props.product.features\n        ? [\n            {\n              title: \"core features\",\n              content: this.props.product.features,\n              open: false,\n            },\n            {\n              title: \"description\",\n              content: this.props.product.description,\n              open: false,\n            },\n            {\n              title: \"delivery & returns\",\n              content: this.props.product.delivery,\n              open: false,\n            },\n            {\n              title: \"care guide\",\n              content: this.props.product.care,\n              open: false,\n            },\n          ]\n        : null,\n    });\n  };\n\n  componentDidUpdate = (prevProps, prevState) => {\n    if (prevState.productFeatures === null) {\n      this.setState({\n        productFeatures: [\n          {\n            title: \"core features\",\n            content: this.props.product.features,\n            open: false,\n          },\n          {\n            title: \"description\",\n            content: this.props.product.description,\n            open: false,\n          },\n          {\n            title: \"delivery & returns\",\n            content: this.props.product.delivery,\n            open: false,\n          },\n          {\n            title: \"care guide\",\n            content: this.props.product.care,\n            open: false,\n          },\n        ],\n      });\n    }\n  };\n\n  handleVariantSelect = (index) => {\n    this.props.handleVariantSelect(index);\n  };\n\n  handleSizeSelect = (size) => {\n    if (this.state.selectedSize === size) {\n      this.setState({\n        selectedSize: null,\n      });\n    } else {\n      this.setState({\n        selectedSize: size,\n      });\n    }\n  };\n\n  handleToggle = (index) => {\n    this.setState((prevState) => ({\n      productFeatures: prevState.productFeatures.map((feature, i) => {\n        if (index === i) {\n          return {\n            ...feature,\n            open: !feature.open,\n          };\n        } else {\n          return {\n            ...feature,\n            open: false,\n          };\n        }\n      }),\n    }));\n  };\n\n  handleAddToCart = () => {\n    const product = {\n      name: this.props.product.name,\n      category: this.props.product.category,\n      collection: this.props.product.collection,\n      color: this.props.product.colors[this.props.variantIndex],\n      id: this.props.match.params.id,\n      image: this.props.product.variants[this.props.variantIndex].images[0].src,\n      price: this.props.product.price,\n      qty: 1,\n      size: this.state.selectedSize,\n    };\n    this.props.addToCart(product, this.props.authenticated);\n    this.setState({\n      selectedSize: null,\n    });\n  };\n\n  render() {\n    const { product, variantIndex, totalReviews } = this.props;\n    const { selectedSize, productFeatures } = this.state;\n    if (product.colors === undefined || product.variants === undefined) {\n      return <h1>LOADING</h1>;\n    } else {\n      return (\n        <div className=\"product-main\">\n          <h1 className=\"product-main__title\">{product.name}</h1>\n          <div className=\"product-main__rating\">\n            {this.props.displayRatingStars(\"light\")}\n            {\n              <a\n              style={{cursor: totalReviews > 0 ? 'pointer' : 'default'}}\n                onClick={() => {\n                  if (totalReviews > 0) {\n                    window.scrollTo({\n                      left: 0,\n                      top: document.body.scrollHeight,\n                      behavior: \"smooth\",\n                    });\n                  }\n                }}\n              >\n                ({totalReviews})\n              </a>\n            }\n          </div>\n          <p className=\"product-main__price\">£{product.price}</p>\n          <div className=\"product-main__variants-container\">\n            <span>STANDARD EDITION:</span> {\"\"}\n            <span>{capitaliseFirstLetter(product.colors[variantIndex])}</span>\n            <div className=\"product-main__variants\">\n              {product.colors.map((color, index) => (\n                <div\n                  key={index}\n                  className={\n                    variantIndex === index\n                      ? \"product-main__variant active\"\n                      : \"product-main__variant\"\n                  }\n                >\n                  <div\n                    className={`product-main__variant-color product-main__variant-color--${color}`}\n                    onClick={() => this.handleVariantSelect(index)}\n                  />\n                </div>\n              ))}\n            </div>\n          </div>\n          <div className=\"product-main__variant-sizes-container\">\n            <span>SELECT SIZE:</span>\n            <div className=\"product-main__variant-sizes\">\n              {product.variants[variantIndex].sizes &&\n                product.variants[variantIndex].sizes.map(\n                  ({ size, stockQty }, index) => (\n                    <button\n                      disabled={stockQty === 0}\n                      key={index}\n                      onClick={() => this.handleSizeSelect(size)}\n                      className={\n                        stockQty > 0\n                          ? \"product-main__variant-size product-main__variant-size--active\"\n                          : \"product-main__variant-size product-main__variant-size--disabled\"\n                      }\n                    >\n                      <p>UK</p>\n                      {size}\n                    </button>\n                  )\n                )}\n            </div>\n          </div>\n          <div className=\"product-main__cart-container\">\n            <button\n              onClick={this.handleAddToCart}\n              disabled={selectedSize == null}\n              className={\n                selectedSize\n                  ? \"product-main__add-to-cart-btn\"\n                  : \"product-main__add-to-cart-btn disabled \"\n              }\n            >\n              {selectedSize ? \"ADD TO CART\" : \"SELECT SIZE\"}\n            </button>\n            <div className=\"product-main__cart\">\n              <p className=\"\">Free delivery and 30 day returns!</p>\n            </div>\n          </div>\n          <div className=\"product-main__highlights-container\">\n            {product.highlights && (\n              <h5>{product.name.toUpperCase()} HIGHLIGHTS</h5>\n            )}\n            <div className=\"product-main__highlights\">\n              {product.highlights &&\n                product.highlights.map((highlight, index) => (\n                  <div key={index} className=\"product-main__hightlight\">\n                    <img src={highlight.img} />\n                    <p>{highlight.text}</p>\n                  </div>\n                ))}\n            </div>\n          </div>\n          <div className=\"product-main__accordion-container\">\n            {productFeatures &&\n              productFeatures.map((feature, index) => (\n                <Accordion\n                  key={index}\n                  index={index}\n                  feature={feature}\n                  handleToggle={this.handleToggle}\n                />\n              ))}\n            <button className=\"accordion__btn\"></button>\n          </div>\n        </div>\n      );\n    }\n  }\n}\n\nconst mapActionsToProps = {\n  addToCart,\n};\n\nconst mapStateToProps = (state) => ({\n  authenticated: state.account.authenticated,\n});\n\nexport default connect(\n  mapStateToProps,\n  mapActionsToProps\n)(withRouter(ProductMain));\n","/Users/seanadamson/Documents/ecommerce-app/src/components/accordion/accordion.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/modal/modal.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/redux/reducers/reviewsReducer.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/redux/actions/reviewActions.js",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/reviews/review-card.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/filters/weather.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/components/products/product-list-item/favourite/favourite.jsx",[],["314","315"],"/Users/seanadamson/Documents/ecommerce-app/src/components/products/product-list-item/sizes/sizes.jsx",[],["316","317"],"/Users/seanadamson/Documents/ecommerce-app/src/components/sort/sort.jsx",[],"/Users/seanadamson/Documents/ecommerce-app/src/pages/checkout/checkout-page.jsx",["318","319","320","321"],"import React from \"react\";\nimport \"./checkout-page.scss\";\nimport { connect } from \"react-redux\";\nimport { Link, Redirect } from \"react-router-dom\";\nimport { ElementsConsumer } from \"@stripe/react-stripe-js\";\nimport CheckoutForm from \"../../components/checkout-form/checkout-form\";\nimport CheckoutLineItem from \".